<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleXmlTransform" resourceType="Unspecified" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
      </handlers>
    </system.webServer>
  </location>
</configuration>